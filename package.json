{
  "name": "aframe-maze-component",
  "version": "0.1.0",
  "description": "aframe maze component",
  "main": "modules/index.js",
  "scripts": {
    "//": "To use: npm run test-nodemon (assumes global location of mocha)",
    "start": "node index.js",
    "test": "mocha --recursive --timeout 20000",
    "test-nodemon": "nodemon /usr/local/bin/mocha --timeout 5000",
    "test-debug": "nodemon /usr/local/bin/mocha -g '@DEBUG' --recursive --timeout 20000",
    "test-jenkins": "mocha --recursive --timeout 20000 -R xunit test/ > test-reports.xml",
    "mac-server": "python -m SimpleHTTPServer 8000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mitchallen/aframe-maze-component.git"
  },
  "author": "Mitch Allen <npm@mitchallen.com> (http://mitchallen.com)",
  "license": "ISC",
  "dependencies": {
    "@mitchallen/maze-generator-square": "^0.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify-derequire": "^0.9.4",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-shell": "^1.3.0",
    "should": "^10.0.0",
    "supertest": "^2.0.0"
  }
